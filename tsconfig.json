{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "downlevelIteration": true,

    "strict": true,
    "noImplicitAny": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "alwaysStrict": true,

    "noImplicitReturns": true,

    "esModuleInterop": true
  },
  "include": ["scripts"],
  "exclude": []
}
